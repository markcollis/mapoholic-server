{
  "name": "mapoholic-server",
  "version": "1.0.0",
  "license": "ISC",
  "description": "Back end for MapOholic orienteering map database",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=\"development\" && node index.js",
    "win-start": "set NODE_ENV=\"development\" && node index.js",
    "start-watch": "export NODE_ENV=\"development\" && nodemon index.js",
    "win-start-watch": "set NODE_ENV=\"development\" && nodemon index.js",
    "local-watch": "export NODE_ENV=\"local\" && nodemon index.js",
    "test": "export NODE_ENV=\"test\" && mocha ./**/*.test.js",
    "test-watch": "nodemon --exec 'yarn test'",
    "mongo": "sudo service mongod start"
  },
  "author": "Mark Collis",
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "expect": "^24.1.0",
    "mocha": "^6.0.1",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "binary-parser": "^1.7.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "jpeg-js": "^0.3.5",
    "jwt-simple": "^0.5.5",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.10",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-fetch": "^3.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pixelmatch": "^5.0.2",
    "pngjs": "^3.4.0",
    "sharp": "^0.22.1",
    "validator": "^11.0.0"
  }
}
